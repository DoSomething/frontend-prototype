{
  "name": "prototype",
  "version": "1.0.0",
  "description": "A proof-of-concept of a lightweight front-end layer for DoSomething.org.",
  "main": "app/server.js",
  "browser": "app/client.js",
  "scripts": {
    "start": "nodemon app/server.js",
    "build": "NODE_ENV=production browserify . | uglifyjs -cm > dist/app.js",
    "watch": "watchify -o dist/app.js -v -d .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "David Furnes <dfurnes@dosomething.org>",
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "dependencies": {
    "babel": "^4.4.5",
    "compression": "^1.4.1",
    "express": "^4.11.2",
    "express-handlebars": "^1.2.1",
    "html5shiv": "^3.7.2",
    "jquery": "^1.11.2",
    "neue": "DoSomething/neue#v6.0.1",
    "react": "^0.12.2",
    "respond.js": "^1.4.2",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "babelify": "^5.0.3",
    "browserify": "^8.1.3",
    "envify": "^3.2.0",
    "nodemon": "^1.3.7",
    "uglify-js": "^2.4.16",
    "watchify": "^2.3.0"
  }
}
